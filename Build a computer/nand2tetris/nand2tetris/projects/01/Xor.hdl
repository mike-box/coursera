/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not (in = a , out = nota);
    Not (in = b , out = notb);
    And (a = a,b = notb, out = w1);
    And (a = b,b = nota, out = w2);
    Or  (a = w1,b = w2, out = out);
}